<?xml version="1.0" encoding="utf-8"?>
<root xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="SpaceSchema.xsd">
	<space>
		<border-layout-container>
			<data>
				<border-layout-data layoutregion="WEST" maxSize="500" minSize="150" size="360" split="true"/>
				<border-layout-data layoutregion="CENTER" split="true"/>
			</data>
			<content>
				<accordion-container>
					<content>
						<widget config="WG Docbase browser"/>
						<widget config="WG Predefined search"/>
						<widget config="WG Favorites"/>
						<widget config="WG Checkout"/>
						<widget config="WG Recycle Bin"/>
					</content>
				</accordion-container>
				<border-layout-container>
					<data>
						<border-layout-data layoutregion="NORTH" size=".8"/>
						<border-layout-data layoutregion="SOUTH" size=".2" split="true"/>
					</data>
					<content>
						<border-layout-container>
							<data>
								<border-layout-data layoutregion="WEST" size=".5"/>
								<border-layout-data layoutregion="EAST" size=".5" split="true"/>
							</data>
							<content>
								<tab-container>
									<content>
										<widget config="WG Doclist eTMF"/>
										<widget config="WG EXT LSCI LSS Doc Viewer (Browse)"/>
										<widget config="WG Properties"/>
									</content>
								</tab-container>
								<tab-container>
									<content>
										<widget config="WG Virtual document"/>
									</content>
								</tab-container>
							</content>
						</border-layout-container>
						<tab-container>
							<content>
								<widget config="WG Locations"/>
								<widget config="WG Versions"/>
								<widget config="WG Renditions"/>
								<widget config="WG Relations"/>
								<widget config="WG Change Request"/>
								<widget config="WG Audit"/>
								<widget config="WG WF Overview"/>
							</content>
						</tab-container>
					</content>
				</border-layout-container>
			</content>
		</border-layout-container>
	</space>
</root>