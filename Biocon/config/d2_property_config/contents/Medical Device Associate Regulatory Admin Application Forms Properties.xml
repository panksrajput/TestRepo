<?xml version="1.0" encoding="utf-8"?>
<dialog id="PropertiesDialog" auto_smartview_edit_mode="false" buttons_right="false" focus="" height="650" initial_invalid="true" resizable="true" signoff_creation="false" signoff_edit="false" signoff_import="false" signoff_intention_dictionary="" signoff_intention_required="false" width="950">
	<content>
		<fieldset label_en="Associate Regulatory Admin Application Forms">
			<label id="domain" condition_visible_create="false" condition_visible_edit="false" condition_visible_import="false"/>
			<label id="artifact_name" condition_visible_create="false" condition_visible_edit="false" condition_visible_import="false"/>
			<combo id="dv_device_family" advancedView_visible="false" assistance_dql="SELECT DISTINCT dv_device_family FROM cd_product_info WHERE a_status != 'Inactive' AND dv_device_family != ' ' ORDER BY 1" assistance_type="dql" asynchronous="true" condition_enabled_edit="false" condition_required_create="true" condition_required_edit="true" condition_required_import="true"/>
			<combo id="dv_device_subfamily" advancedView_visible="false" assistance_dql="SELECT DISTINCT dv_device_subfamily FROM cd_product_info WHERE a_status != 'Inactive' AND dv_device_family = '$value(dv_device_family)' AND dv_device_subfamily != ' ' ORDER BY 1" assistance_type="dql" asynchronous="false" autoselect_single="true" condition_enabled_edit="false" condition_required_create="true" condition_required_edit="true" condition_required_import="true" condition_visible="((getValue(domain).notEquals('Regulatory Correspondence'))||(getValue(artifact_name).equals('Submission Receipt Confirmation')))"/>
			<list id="dv_device_code" assistance_dql="SELECT DISTINCT dv_device_code FROM cd_product_info WHERE a_status != 'Inactive' AND dv_device_family = '$value(dv_device_family)' AND dv_device_subfamily = '$value(dv_device_subfamily)' AND ANY dv_device_code != ' ' ORDER BY 1 enable (ROW_BASED)" assistance_type="dql" asynchronous="false" condition_required_create="true" condition_required_edit="true" condition_required_import="true" rows="6" sorted="true"/>
			<comment advancedView_visible="false" font_color="#646464" html_content="true" label_en="Select &quot;Regulatory Application Registration Form&quot; from the available list to associate with the document. It will create Regulatory Application Form relation."/>
			<grid advancedView_visible="false" condition_visible="(getValue(artifact_name).notEquals('Submission Receipt Confirmation'))" grid_height="250">
				<combo id="application_descriptions" advancedView_enabled="false" advancedView_required="false" advancedView_visible="false" assistance_dictionary="Regulatory Application Forms with Registered Family and Subfamily" assistance_dictionary_alias="auto" assistance_type="dictionary" asynchronous="true" cell_width="145" condition_required="(getValue(domain).equals('Device Regulatory'))" condition_required_create="true" condition_required_edit="true" condition_required_import="true" label_en="Application Description" no_empty_cell="false"/>
				<combo id="application_numbers" advancedView_visible="false" assistance_dql="select application_num from cd_reg_admin_info where application_description='$value(application_descriptions)'" assistance_type="dql" cell_width="100" condition_enabled_create="false" condition_enabled_edit="false" condition_enabled_import="false" condition_visible="(getValue(domain).equals('Device Regulatory'))" label_en="Application No"/>
				<combo id="regions" advancedView_visible="false" assistance_dictionary="Geographic Region For Registered Applications" assistance_dictionary_alias="auto" assistance_type="dictionary" cell_width="80" condition_enabled_create="false" condition_enabled_edit="false" condition_enabled_import="false" condition_visible="(getValue(domain).equals('Device Regulatory'))" label_en="Region"/>
				<combo id="application_types" advancedView_visible="false" assistance_dql="select application_type from cd_reg_admin_info where application_description='$value(application_descriptions)'" assistance_type="dql" cell_width="80" condition_enabled_create="false" condition_enabled_edit="false" condition_enabled_import="false" condition_visible="(getValue(domain).equals('Device Regulatory'))" label_en="Type"/>
				<combo id="country_codes" advancedView_visible="false" assistance_dictionary="Submission Country For Registered Applications" assistance_dictionary_alias="auto" assistance_type="dictionary" cell_width="80" condition_enabled_create="false" condition_enabled_edit="false" condition_enabled_import="false" condition_visible="(getValue(domain).equals('Device Regulatory'))" label_en="Country"/>
				<combo id="submission_descriptions" advancedView_visible="false" assistance_dql="select distinct submission_description from cd_reg_submission_info where application_description='$value(application_descriptions)' and a_status='Active' order by 1" assistance_type="dql" cell_width="170"/>
				<combo id="submission_types" assistance_dictionary="Regulatory Submission for Application registration form" assistance_dictionary_alias="display_value" assistance_type="dictionary" asynchronous="true" cell_width="110" label_en="Submission Type"/>
				<combo id="submission_numbers" advancedView_enabled="false" advancedView_visible="false" assistance_dql="select distinct submission_number from cd_reg_submission_info where application_description='$value(application_descriptions)' and submission_description='$value(submission_descriptions)' and submission_type='$value(submission_types)'  order by 1" assistance_type="dql" cell_width="120" label_en="Submission No"/>
				<combo id="submission_info_object_id" assistance_dql="SELECT r_object_id FROM cd_reg_submission_info WHERE  submission_number='$value(submission_numbers)' AND submission_description='$value(submission_descriptions)'" assistance_type="dql" autoselect_single="true" cell_width="0" condition_visible_create="false" condition_visible_edit="false" condition_visible_import="false"/>
			</grid>
		</fieldset>
	</content>
	<buttons>
		<button id="buttonOk" action="validDialog()"/>
		<button id="buttonCancel" action="cancelDialog()"/>
	</buttons>
</dialog>