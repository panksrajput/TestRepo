<?xml version="1.0" encoding="utf-8"?>
<dialog id="PropertiesDialog" buttons_right="false" focus="" height="300" resizable="true" signoff_creation="false" signoff_edit="false" signoff_import="false" signoff_intention_dictionary="" signoff_intention_required="false" width="400">
	<content>
		<fieldset label_en="Set Planned Effective Date">
			<label id="object_name"/>
			<label id="r_version_label"/>
			<label id="a_status" assistance_dictionary="Domain Status DQL Dictionary" assistance_dictionary_alias="display_value" assistance_type="dictionary"/>
			<separator/>
			<date id="planned_effective_date" advancedView_enabled="false" advancedView_required="false" condition_enabled="((getValue(a_status).equals('Release Pending'))||(getValue(a_status).equals('Draft'))||(getValue(a_status).notEquals('Effective')))" condition_validation="getValue(planned_effective_date).isGreaterOrEqualsThan('$TODAY')"/>
			<memo id="comments" advancedView_enabled="false" advancedView_required="false" advancedView_visible="false" condition_enabled="(getValue(a_status).equals('Release Pending'))" condition_required="(getValue(a_status).equals('Release Pending'))" condition_required_edit="true" condition_visible="(getValue(a_status).equals('Release Pending'))" label_en="Reason For Change"/>
		</fieldset>
	</content>
	<buttons>
		<button id="buttonOk" action="validDialog()"/>
		<button id="buttonCancel" action="cancelDialog()"/>
	</buttons>
</dialog>