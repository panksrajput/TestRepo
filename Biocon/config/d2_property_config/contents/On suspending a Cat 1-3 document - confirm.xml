<?xml version="1.0" encoding="utf-8"?>
<dialog id="PropertiesDialog" buttons_right="false" focus="" height="240" resizable="true" signoff_creation="false" signoff_edit="false" signoff_import="false" signoff_intention_dictionary="" signoff_intention_required="false" width="450">
	<content>
