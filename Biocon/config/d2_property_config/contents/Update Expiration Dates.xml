<?xml version="1.0" encoding="utf-8"?>
<dialog id="PropertiesDialog" buttons_right="false" focus="" height="300" resizable="true" signoff_creation="false" signoff_edit="false" signoff_import="false" signoff_intention_dictionary="" signoff_intention_required="false" width="430">
	<content>
		<tabs>
			<tab label_en="Set Expiry Review Dates">
				<text id="category" condition_enabled_create="false" condition_enabled_edit="false" condition_enabled_import="false" condition_visible_create="false" condition_visible_edit="false" condition_visible_import="false"/>
				<text id="a_status" condition_enabled_create="false" condition_enabled_edit="false" condition_enabled_import="false" condition_visible_create="false" condition_visible_edit="false" condition_visible_import="false"/>
				<date id="approved_date" condition_enabled_create="false" condition_enabled_edit="false" condition_enabled_import="false" label_en="Approved Date"/>
				<date id="expiration_date" advancedView_enabled="false" advancedView_visible="false" condition_enabled_create="false" condition_enabled_edit="false" condition_enabled_import="false" condition_validation="getValue(expiration_date).isGreaterOrEqualsThan('$TODAY')&amp;&amp;getValue(expiration_date).isGreaterOrEqualsThan('getValue(next_periodic_review_date)')" condition_visible="(getValue(category).equals('2'))" label_en="Expiration Review Due Date"/>
				<date id="expiry_review_date" advancedView_visible="false" condition_enabled_create="false" condition_enabled_import="false" condition_validation="getValue(expiry_review_date).isGreaterOrEqualsThan('$TODAY')&amp;&amp;getValue(expiry_review_date).isLessOrEqualsThan('getValue(expiration_date)')" condition_visible="(getValue(category).equals('2'))" label_en="Next Expiry Review Date" validation_message_en="Next Periodic Review Date must be Greater than or equal to today and must be less than or equal to Periodic Review Due Date."/>
				<date id="last_expiry_review_date" advancedView_enabled="false" advancedView_visible="false" condition_enabled="(getValue(category).equals('2'))" condition_enabled_create="false" condition_enabled_edit="false" condition_enabled_import="false" condition_visible="(getValue(category).equals('2'))" label_en="Last Expiry Review Date"/>
				<separator/>
				<list id="review_notif_recipients" assistance_dql="select group_name from dm_group where any i_supergroups_names='$value(group_name)_doc_coordinators' union select i_all_users_names from dm_group where group_name = '$value(group_name)_doc_coordinators' Union SELECT group_name FROM dm_group WHERE group_name IN (select groups_names from dm_group where group_name='$value(group_name)_doc_coordinators')" assistance_type="dql" asynchronous="false" condition_required_edit="true" label_en="Expiry Review Recipients" rows="4" sorted="true"/>
				<memo id="comments" advancedView_enabled="false" advancedView_required="false" advancedView_visible="false" condition_enabled="(getValue(a_status).equals('Effective'))" condition_required="(getValue(a_status).equals('Effective'))" condition_required_edit="true" condition_visible="(getValue(a_status).equals('Effective'))" label_en="Reason For Change"/>
			</tab>
		</tabs>
	</content>
	<buttons>
		<button id="buttonOk" action="validDialog()"/>
		<button id="buttonCancel" action="cancelDialog()"/>
	</buttons>
</dialog>